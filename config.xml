<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.phonegap.prepop"
        versionCode = "10" 
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>Prepop DB</name>

    <description>
     Ejemplo de base de datos prellena
    </description>


	<!-- ICONO -->
		<icon src="icono.png" gap:platform="android" gap:density="1dpi" />
			<gap:splash src="icono.png" gap:platform="android" gap:density="1dpi" />
			<gap:splash src="icono.png" gap:platform="android" gap:density="mdpi" />
			<gap:splash src="icono.png" gap:platform="android" gap:density="hdpi" />
			<gap:splash src="icono.png" gap:platform="android" gap:density="xhdpi" />
			

    <preference name="phonegap-version" value="3.7.0" />
	
    <author href="https://build.phonegap.com" email="support@phonegap.com">
      ICEon
   </author>
	
		<gap:plugin name="org.apache.cordova.vibration" version="0.3.0" />
